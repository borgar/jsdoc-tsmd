{
  "extends": [ "@borgar/eslint-config" ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false,
  },
  "rules": {
    "no-undefined": "off",
    "quotes": "off",
    "no-console": "off",
    "newline-per-chained-call": "off",
    "max-len": "off",
    "no-multiple-empty-lines": "off",
  },
  "globals": {
    "console": true,
    "process": true,
    "require": true,
    "exports": true,
    "module": true,
  }
}
